<?xml version="1.0"?>

<selectors>

  <selector name="arithmetic promotion expression 1" selects="all">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>1 = 1.0</expression>
  </selector>

  <selector name="arithmetic promotion expression 2" selects="none">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>1 = 1.1</expression>
  </selector>

  <selector name="arithmetic promotion expression 3" selects="all">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>1.0 = 1</expression>
  </selector>

  <selector name="arithmetic promotion expression 4" selects="none">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <expression>1.1 = 1.0</expression>
  </selector>

  <selector name="arithmetic promotion expression 5" selects="all">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>1 &lt; 1.1</expression>
  </selector>

  <selector name="arithmetic promotion expression 6" selects="all">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>1.1 &gt; 1</expression>
  </selector>

  <selector name="arithmetic promotion expression 7" selects="all">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>1 &lt;= 1.1</expression>
  </selector>

  <selector name="arithmetic promotion expression 8" selects="all">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>1.1 &gt;= 1</expression>
  </selector>

  <selector name="arithmetic promotion expression 9" selects="all">
    <requirementId>selector.operator.arithmetic.promotion</requirementId>
    <requirementId>selector.operator.comparison.types</requirementId>
    <expression>3*3.3 &gt; 9 and 3.3*3 &lt; 10.0</expression>
  </selector>
     
</selectors>