<?xml version="1.0"?>

<selectors>

  <selector name="true in expression 1" selects="all">
    <requirementId>selector.operator.in</requirementId>
    <requirementId>selector.expression</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                value="France" />
    <expression>Country in ('France')</expression>
  </selector>

  <selector name="false in expression 1" selects="none">
    <requirementId>selector.operator.in</requirementId>
    <requirementId>selector.expression</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                value="France" />
    <expression>Country NOT IN ('France')</expression>
  </selector>

  <selector name="true in expression 2" selects="all">
    <requirementId>selector.operator.in</requirementId>
    <requirementId>selector.expression</requirementId>
