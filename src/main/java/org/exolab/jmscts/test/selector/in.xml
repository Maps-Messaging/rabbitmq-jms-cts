    <properties:property name="Country" type="java.lang.String" 
                value="France" />
    <expression>Country in ('FRANCE')</expression>
  </selector>

  <selector name="unknown in expression 1" selects="none">
    <requirementId>selector.operator.in</requirementId>
    <requirementId>selector.expression</requirementId>
    <requirementId>selector.values.null</requirementId>
    <expression>Country in ('France')</expression>
  </selector>

  <selector name="invalid in expression 1" selects="none" valid="false">
    <requirementId>selector.validation</requirementId>
    <expression>age in (21, 22, 23)</expression>
  </selector>

  <selector name="invalid in expression 2" selects="none" valid="false">
