<?xml version="1.0"?>

<selectors>

  <selector name="true condition expression 1" selects="all">
    <requirementId>selector.expression.condition</requirementId>
    <!-- (true or (test = true)) -->
    <properties:property name="test" type="java.lang.Boolean" value="false" />
    <expression>true or test = true</expression>
  </selector>

  <selector name="true condition expression 2" selects="all">
    <!-- unknown property test -->
    <requirementId>selector.expression.condition</requirementId>
    <expression>true or test = true</expression>
  </selector>

  <selector name="false condition expression 1" selects="none">
    <requirementId>selector.expression.condition</requirementId>
    <!-- (false and (test = true)) -->
    <properties:property name="test" type="java.lang.Boolean" value="false" />
    <expression>false and test = true</expression>
  </selector>

  <selector name="false condition expression 2" selects="none">
    <!-- unknown property test -->
    <requirementId>selector.expression.condition</requirementId>
    <expression>false and test = true</expression>
  </selector>

  <selector name="false condition expression 3" selects="none">
    <requirementId>selector.evaluation.order</requirementId>
    <requirementId>selector.expression.condition</requirementId>
    <expression>(not (((true or false)) and true))</expression>
  </selector>

  <selector name="unknown condition expression 1" selects="none">
    <requirementId>selector.expression.condition</requirementId>
    <requirementId>selector.values.null</requirementId>
    <expression>dummy = dummy</expression>
  </selector>

  <selector name="unknown condition expression 2" selects="none">
    <requirementId>selector.expression.condition</requirementId>
    <requirementId>selector.values.null</requirementId>
    <expression>dummy &lt;&gt; dummy</expression>
  </selector>

  <selector name="unknown condition expression 3" selects="none">
    <requirementId>selector.expression.condition</requirementId>
    <requirementId>selector.values.null</requirementId>
    <expression>dummy &lt; dummy</expression>
  </selector>

  <selector name="unknown condition expression 4" selects="none">
    <requirementId>selector.expression.condition</requirementId>
    <requirementId>selector.values.null</requirementId>
    <expression>dummy &gt; dummy</expression>
  </selector>

  <selector name="unknown condition expression 5" selects="none">
    <requirementId>selector.expression.condition</requirementId>
    <requirementId>selector.values.null</requirementId>
    <expression>dummy &lt;= dummy</expression>
  </selector>

  <selector name="unknown condition expression 6" selects="none">
    <requirementId>selector.expression.condition</requirementId>
    <requirementId>selector.values.null</requirementId>
    <expression>dummy &gt;= dummy</expression>
  </selector>

  <selector name="invalid condition expression 1" selects="none" valid="false">
    <requirementId>selector.validation</requirementId>
    <expression>false - true</expression>
  </selector>

  <selector name="invalid condition expression 2" selects="none" valid="false">
    <requirementId>selector.validation</requirementId>
    <expression>true + false</expression>
  </selector>

  <selector name="invalid condition expression 3" selects="none" valid="false">
    <requirementId>selector.validation</requirementId>
    <expression>true / false</expression>
  </selector>

  <selector name="invalid condition expression 4" selects="none" valid="false">
    <requirementId>selector.validation</requirementId>
    <expression>true * false</expression>
  </selector>

  <selector name="invalid condition expression 5" selects="none" valid="false">
    <requirementId>selector.validation</requirementId>
    <expression>true not true</expression>
  </selector>
     
</selectors>
