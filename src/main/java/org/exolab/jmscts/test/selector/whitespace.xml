<?xml version="1.0"?>

<selectors>

  <!-- space character tests -->
  <selector name="whitespace expression 1" selects="all">
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country&#x20;='France'</expression>
  </selector>

  <selector name="whitespace expression 2" selects="none">
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country='France&#x20;'</expression>
  </selector>

  <!-- tab character tests -->
  <selector name="whitespace expression 3" selects="all">
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country&#x9;=&#x9;'France'</expression>
  </selector>
 
  <selector name="whitespace expression 4" selects="none">
    <!-- embedded tab in string literal -->
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country='&#x9;France'</expression>
  </selector>

  <!-- line feed tests -->
  <selector name="whitespace expression 5" selects="all">
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country&#xA;=&#xA;'France'&#xA;</expression>
  </selector>
  
  <selector name="whitespace expression 6" selects="none" valid="false">
    <!-- embedded line feed in string literal -->
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country='&#xA;France'</expression>
  </selector>

  <!-- carriage return tests -->
  <selector name="whitespace expression 7" selects="all">
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country&#xD;=&#xD;'France'&#xD;</expression>
  </selector>

  <selector name="whitespace expression 8" selects="none" valid="false">
    <!-- embedded carriage return in string literal -->
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country='&#xD;France'</expression>
  </selector>

  <!-- cr/lf tests -->
  <selector name="whitespace expression 9" selects="all">
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country&#xD;&#xA;=&#xD;&#xA;'France'&#xD;&#xA;</expression>
  </selector>

  <selector name="whitespace expression 10" selects="none" valid="false">
    <!-- embedded cr/lf in string literal -->
    <requirementId>selector.whitespace</requirementId>
    <properties:property name="Country" type="java.lang.String" 
                         value="France" />
    <expression>Country='&#xD;&#xA;France'</expression>
  </selector>
    
</selectors>
