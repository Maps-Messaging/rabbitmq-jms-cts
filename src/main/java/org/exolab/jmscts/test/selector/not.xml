<?xml version="1.0"?>

<selectors>

  <selector name="true not expression 1" selects="all">
    <requirementId>selector.operator.not</requirementId>
    <requirementId>selector.expression</requirementId>
    <expression>not false</expression>
  </selector>

  <selector name="false not expression 1" selects="none">
    <requirementId>selector.operator.not</requirementId>
    <requirementId>selector.expression</requirementId>
    <expression>not true</expression>
  </selector>

  <selector name="unknown not expression 1" selects="none">
    <requirementId>selector.operator.not</requirementId>
    <requirementId>selector.expression</requirementId>
    <expression>not dummy</expression>
  </selector>

  <selector name="invalid not expression 1" selects="none" valid="false">     
    <requirementId>selector.validation</requirementId>
    <expression>not</expression>
  </selector>

  <selector name="invalid not expression 2" selects="none" valid="false">     
    <requirementId>selector.validation</requirementId>
    <expression>true not</expression>
  </selector>

</selectors>
